@model CodeNoteDto

@{
    var title = "Kod Düzenle";
    ViewData["Title"] = title;
}

@Html.Raw(bread.BreadCrumb(
    new List<Bc>() {
        new Bc("Dashboard", Url.Action("Index","Home")),
        new Bc("Liste", Url.Action("Index","CodeNote")),
        new Bc(title , "")
    }
))

@Html.Raw(pageHeader.MainHeader(
    title,
    "",
    "Listeye Dön",
    Url.Action("Index","CodeNote")
))


<div class="content">
    <div class="row">
        <div class="col-md-12 col-sm-12">
            <div class="card">
                <div class="card-header">
                    <h5 class="card-title">@title</h5>
                </div>
                <div class="card-body">
                    @await Component.InvokeAsync("CodeNoteForm", new { action = "Edit", Model = Model })
                </div>
            </div>
        </div>
    </div>
</div>
 
 

@section Styles
{
    @await Html.PartialAsync("Components/CodeNoteForm/_CodeNoteFormStylesPartial")
}

@section Scripts
{
  @await Html.PartialAsync("Components/CodeNoteForm/_CodeNoteFormScriptsPartial")
}